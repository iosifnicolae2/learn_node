<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <% include head %>
    <% include scripts %>
  </head>
  <body>
    <h2><%= title %></h2>

    <ul>
      <%  menu_of_the_day.forEach(function(dish, index, array){ %>
        <% if(dish.enabled==true){ %>
        <h3><%=dish.name %></h3>
        <img src="<%=dish.image %>"></img>
        <p><% dish.description %></p>
        <p>Price: <%=dish.price %></p>
        <p>Quantity: <%=dish.quantity %></p></div>
          <%}
        })%>


    </ul>


  </body>
</html>
